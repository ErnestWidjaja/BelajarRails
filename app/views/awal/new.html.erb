<%= form_for(@data) do |f| %>
    <div class="form-group row justify-content-beetween">
        <%= f.label :nama, :class => "col-sm-2" %>
        <%= f.text_field :nama, :class => "form-control" %>
    </div>
    <div class="form-group row justify-content-beetween">
        <%= f.label :status, :class => "col-sm-2" %>
        <%= f.text_field :status, :class => "form-control" %>
    </div>
    <div class="form-group row justify-content-beetween">
        <%= f.label :umur, :class => "col-sm-2" %>
        <%= f.text_field :umur, :class => "form-control" %>
    </div>
    <div class="row">
        <div class="col-sm-7">
            <%= f.submit "Save", :class => "btn btn-primary" %>
        </div>
    </div>
<% end %>